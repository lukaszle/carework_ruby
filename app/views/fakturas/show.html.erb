<%- model_class = Faktura -%>
<div class="page-header">
  <h1><%=t '.title', :default => Faktura %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:id) %></dt>
      <dd><pre class="prettyprint"><%= @faktura.id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tytul) %></dt>
      <dd><pre class="prettyprint"><%= @faktura.tytul %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:data) %></dt>
      <dd><pre class="prettyprint"><%= @faktura.data %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:wyslana) %></dt>
      <dd><pre class="prettyprint"><%= @faktura.wyslana %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              fakturas_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_faktura_path(@faktura), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              faktura_path(@faktura),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

